{
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://localhost:8800/api/v1"
      }
    },
    "preview": {
      "channel": "preview",
      "ios": {
        "simulator": true
      },
      "env": {
        "API_URL": "https://exuberant-pear-stockings.cyclic.app"
      }
    },
    "production": {
      "channel": "development",
      "env": {
        "API_URL": "https://exuberant-pear-stockings.cyclic.app/api/v1"
      }
    }
  }
}
